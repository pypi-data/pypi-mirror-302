API_BASE_URL=http://localhost:5173/api

OIDC_PROVIDERS=gitlab,github,dex

# gitlab
GITLAB_CLIENT_ID=...
GITLAB_CLIENT_SECRET=...
GITLAB_SERVER_METADATA_URL=https://gitlab.com/.well-known/openid-configuration
GITLAB_SCOPE=openid profile

# github
GITHUB_CLIENT_ID=...
GITHUB_CLIENT_SECRET=...
